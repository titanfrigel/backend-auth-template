{
  "$schema": "http://json.schemastore.org/template",
  "author": "GaÃ«tan Kling",
  "classifications": ["Clean Architecture"],
  "identity": "BackendAuth.FeatureTemplate",
  "name": "Feature Template",
  "shortName": "feature",
  "sourceName": "BackendAuthTemplate",
  "preferNameDirectory": false,
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "symbols": {
    "FeatureName": {
      "type": "parameter",
      "datatype": "text",
      "isRequired": true,
      "replaces": "FeatureName",
      "fileRename": "FeatureName",
      "displayName": "Feature Name",
      "description": "The name of the feature to be created."
    },
    "EntityName": {
      "type": "parameter",
      "datatype": "text",
      "isRequired": true,
      "replaces": "EntityName",
      "fileRename": "EntityName",
      "displayName": "Entity Name",
      "description": "The name of the entity to be created."
    },

    "featureName": {
      "type": "derived",
      "valueSource": "FeatureName",
      "valueTransform": "firstLowerCase",
      "replaces": "featureName",
      "fileRename": "featureName"
    },
    "entityName": {
      "type": "derived",
      "valueSource": "EntityName",
      "valueTransform": "firstLowerCase",
      "replaces": "entityName",
      "fileRename": "entityName"
    }
  }
}
